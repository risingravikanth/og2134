<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.oganalysis.entities">
	<class name="CrudeOil">
		<id name="id">
			<generator class="increment"/>
		</id>	
			<property name="field" type="string"/> 
			<property name="region" type="string"/>
			<property name="country" type="string"/>
			<property name="location" type="string"/>
			<property name="basin" type="string"/>
			<property name="status" type="string"/>
			<property name="onshoreOrOffshore" type="string"/>
			<property name="productionStartDate" type="date"/>
			<property name="currentOperator" type="string"/>
			<property  name="currentOwners" type="string"/>
			<property name="currentOwnership" type="double"/>
			<property name="historicOperator" type="string"/>
			<property name="historicOwners" type="string"/>
			<property name="historicOwnership" type="double"/>
			<property name="historicEquityYear" type="int"/>
			<property name="sourceEquity" type="string"/>
			<property name="year2005" type="double"/>
			<property name="year2006" type="double"/>
			<property name="year2007" type="double"/>
			<property name="year2008" type="double"/>
			<property name="year2009" type="double"/>
			<property name="year2010" type="double"/>
			<property name="year2011" type="double"/>
			<property name="year2012" type="double"/>
			<property name="year2013" type="double"/>
			<property name="year2014" type="double"/>
			<property name="notes" type="string"/>
	</class>
	
</hibernate-mapping>	