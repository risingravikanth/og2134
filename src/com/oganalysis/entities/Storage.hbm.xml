<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.oganalysis.entities">
	<class name="Storage">
		<id name="id">
			<generator class="increment"/>
		</id>	
		<property name="tankFarm" type="string"/>
		<property name="region" type="string"/>
		<property name="country" type="string"/> 
		<property name="location" type="string"/>
		<property name="status" type="string"/>
		<property name="commencementDate" type="date"/>
		<property name="commencementSource" type="string"/>
		<property name="currentOperator" type="string"/>
		<property name="currentOwners" type="string"/>
		<property name="currentOwnership" type="double"/>
		<property name="ownershipNotes" type="string"/>
		<property name="historicOperator" type="string"/>
		<property name="historicOwners" type="string"/>
		<property name="historicOwnership" type="double"/>
		<property name="historicalEquityYear" type="int"/>
		<property name="equitySource" type="string"/>
		<property name="productsStored" type="string"/>
		<property name="modeOfAccess" type="string"/>
		<property name="capacityM3" type="double"/>
		<property name="capacityYear" type="int"/>
		<property name="tanks" type="double"/>
		<property name="tankSizeRange_min_m3" type="double"/>
		<property name="tankSizeRange_max_m3" type="double"/>
		<property name="capacitySource" type="string"/>
		<property name="capitalInvestment" type="double"/>
		<property name="capexSource" type="string"/>
		<property name="notes" type="string"/>		
	</class>
	
</hibernate-mapping>	